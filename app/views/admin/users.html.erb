
<h1>Admin Panel - User Management</h1>
<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Name</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.profile&.name %></td>
      </tr>
    <% end %>
  </tbody>
</table>


<BR>
<%= link_to "Usre management", admin_users_path%>

<%= link_to "Manage Courses", admin_courses_path %>

