    <div class="mb-4 flex flex-col sm:flex-row w-full lg:w-[50%] justify-between">
      <label for="lead source" class=" block mb-2 text-lg font-medium text-[#000]">Lead Source</label>
      <div class="w-full sm:w-[55%] flex flex-col">
        <%= f.text_field :lead_source, class: "validate w-full bg-[#ffffff] border border-[#cccccc] text-[#818181] text-base font-medium rounded-lg block p-2.5 focus:ring-0 focus:ring-offset-0 focus:border-gray-200", autocomplete: "off" %>
        <div class="text-red-600 text-sm mt-1" id="emergency-contact-number-error"></div>
        <% if enquire&.errors&.any? %>
          <% if enquire.errors[:lead_source].any? %>
            <div class="text-red-600 text-sm mt-1">
              <%= enquire.errors.full_messages_for(:lead_source).join(', ') %>
            </div>
          <% end %>
        <% end %>
      </div>
    </div>
  </div>
