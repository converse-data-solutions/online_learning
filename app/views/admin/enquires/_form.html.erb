  <div class= "flex w-[100%] gap-[50px] flex-col lg:flex-row">
    <div class="mb-4 flex flex-col sm:flex-row w-full lg:w-[50%] justify-between">
      <label for="remarks" class=" block mb-2 text-lg font-medium text-[#000]">Remarks</label>
      <div class="w-full sm:w-[55%] flex flex-col">
        <%= f.text_field :remarks, class: "validate w-full bg-[#ffffff] border border-[#cccccc] text-[#818181] text-base font-medium rounded-lg block p-2.5 focus:ring-0 focus:ring-offset-0 focus:border-gray-200", autocomplete: "off" %>
      </div>
    </div>
    <div class="mb-4 flex flex-col sm:flex-row w-full lg:w-[50%] justify-between">
      <label for="sales person" class=" block mb-2 text-lg font-medium text-[#000]">Sales Person</label>
      <div class="w-full sm:w-[55%] flex flex-col">
        <%= f.text_field :sales_person, class: "validate w-full bg-[#ffffff] border border-[#cccccc] text-[#818181] text-base font-medium rounded-lg block p-2.5 focus:ring-0 focus:ring-offset-0 focus:border-gray-200", autocomplete: "off" %>
