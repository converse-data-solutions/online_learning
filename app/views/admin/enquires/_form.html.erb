      <div class="w-full sm:w-[55%] flex flex-col relative">
        <%= f.select(:status, Enquire.statuses.keys.map { |type| [type.humanize, type] }, { prompt: "Select Status" }, { class: "bg-[#fff] border-[1px] foucus:ring-0 text-[#818181] text-[16px] font-medium rounded-lg outline-none block w-full p-2.5 rounded-[7px] bg-[#fff] text-[#818181] filter-handle custom-select select-widht status_validation" }) %>
        <div class="text-red-600 text-sm mt-1" id="enquire_status_error"></div>
        <% if enquire&.errors&.any? %>
          <% if enquire.errors[:status].any? %>
            <div class="text-red-600 text-sm mt-1">
              <%= enquire.errors.full_messages_for(:status).join(', ') %>
            </div>
          <% end %>
        <% end %>
      </div>
    </div>
