                  <div class="text-red-600 text-sm mt-1">
                    <%= enquire.errors.full_messages_for(:location).join(', ') %>
                  </div>
                <% end %>
              <% end %>
            </div>
          </div>
        </div>
        <div class= "flex w-[100%] gap-[50px] flex-col lg:flex-row">
          <div class="mb-4 flex flex-col sm:flex-row w-full lg:w-[50%] justify-between">
            <label for="timeslot" class="block mb-2 text-lg font-medium text-[#000]">Timeslot</label>
            <div class="w-full sm:w-[55%] flex flex-col relative">
              <%= f.text_field :timeslot, class: "validate w-full bg-[#ffffff] border border-[#cccccc] text-[#818181] text-base font-medium rounded-lg block p-2.5 focus:ring-0 focus:ring-offset-0 focus:border-gray-200", autocomplete: "new-contact" %>
              <div class="text-red-600 text-sm mt-1" id="contact-confirmation-error"></div>
              <% if enquire&.errors&.any? %>
                <% if enquire.errors[:timeslot].any? %>
                  <div class="text-red-600 text-sm mt-1">
                    <%= enquire.errors.full_messages_for(:timeslot).join(', ') %>
                  </div>
                <% end %>
              <% end %>
            </div>
          </div>
          <div class="mb-4 flex flex-col sm:flex-row w-full lg:w-[50%] justify-between">
            <label for="status" class="block mb-2 text-lg font-medium text-[#000]">Status</label>
            <div class="w-full sm:w-[55%] flex flex-col relative">
              <%= f.text_field :status, class: "validate w-full bg-[#ffffff] border border-[#cccccc] text-[#818181] text-base font-medium rounded-lg block p-2.5 focus:ring-0 focus:ring-offset-0 focus:border-gray-200", autocomplete: "new-contact" %>
              <div class="text-red-600 text-sm mt-1" id="contact-confirmation-error"></div>
              <% if enquire&.errors&.any? %>
                <% if enquire.errors[:status].any? %>
