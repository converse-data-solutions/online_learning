  <%= form_for Lesson.new, url: admin_lessons_path, method: :post, html: { class: "custom-lesson", id: "lesson-admin-form" }, format: :turbo_stream do |f| %>
  <td><%= f.hidden_field :section_id, value: ( Course.last.sections.first&.id || section_id ) %></td>
  <%= f.text_field :title, id:"title", class: "w-[110px] sm:w-[110px] md:w-[110px] lg:w-[20%] xl:w-[20%] bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-0 focus:ring-offset-0 focus:border-gray-200 block p-2.5" %>
  <%= f.text_field :description, class: 'w-[125px] sm:w-[125px] md:w-[125px] lg:w-[25%] xl:w-[25%] bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg outline-none focus:ring-0 focus:ring-offset-0 focus:border-gray-200 block  p-2.5 rounded-[7px]' %>
  <div class="clipUpload w-[25%]">
    <svg class="fileUploadIcon" width="22" height="22" viewBox="0 0 22 22" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
      <rect width="22" height="22" fill="url(#pattern0)"/>
      <defs>
        <pattern id="pattern0" patternContentUnits="objectBoundingBox" width="1" height="1">
          <use xlink:href="#image0_559_9489" transform="scale(0.015625)"/>
        </pattern>
        <image id="image0_559_9489" width="64" height="64" xlink:href="data:image/png;base64,"/>
      </defs>
    </svg>
    <%= f.file_field :clip %>
    <p class="clip text-[#0bc08b] w-[170px] truncate">Attach Clip</p>
  </div>
  <div class="fileUploadWrap w-[20%]">
    <svg class="fileUploadIcon" width="22" height="22" viewBox="0 0 22 22" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
      <rect width="22" height="22" fill="url(#pattern0)"/>
      <defs>
        <pattern id="pattern0" patternContentUnits="objectBoundingBox" width="1" height="1">
          <use xlink:href="#image0_559_9489" transform="scale(0.015625)"/>
        </pattern>
        <image id="image0_559_9489" width="64" height="64" xlink:href="data:image/png;base64,"/>
      </defs>
    </svg>
    <%= f.file_field :attachments, multiple: true %>
    <p class="attachments text-[#0bc08b] w-[125px] truncate">Attach Files</p>
  </div>
  <%= f.submit "submit", type: "image", src: asset_path("checkmark.png"), class: "mt-[5px] ml-[25px]" %>
<% end %>
<script>
  $("#lesson-modal-close-btn").click();
  $("#lesson-detele-modal-btn").click();
</script>