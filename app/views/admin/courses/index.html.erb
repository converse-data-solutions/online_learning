                              <thead class="text-xs text-[#000] bg-[#EDEDED] dark:text-gray-400">
                                <tr>
                                  <th scope="col" class="px-6 py-5 font-medium text-[18px]">
                                    Section
                                  </th>
                                  <th scope="col" class="px-6 py-5 font-medium text-[18px]">
                                    Description 
                                  </th>
                                  <th scope="col" class="px-6 py-5 font-medium text-[18px]">
                                    Action
                                  </th>
                                </tr>
                              </thead>
                              <tbody class="overflow-auto filter-table-data" id="sections-body">
                                <% @sections.each do |section| %>
                                  <%= render partial: "admin/sections/table", locals: {section: section} %>
                                <% end %>
                                <tr class="display-section hidden bg-[#EDEDED]" id="section_create_form">
                                  <%= form_for Section.new, url: admin_course_sections_path(course_id: Course.last.id), method: :post, html: { class: "custom-section", id: "stepper_section-form" } do |f| %>
                                    <div class=""><%= f.hidden_field :course_id, value: Course.last.id, class: "hide_course" %></div>
                                    <td>
                                      <div class="">
                                        <%= f.text_field :title, class: "hide_title w-full bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-0 focus:ring-offset-0 focus:border-gray-200", id: "test_id" %>
                                      </div>
                                    </td>
                                    <td>
                                      <div class="">
                                        <%= f.text_field :description, class: 'hide_description w-full bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg block focus:ring-0 focus:ring-offset-0 focus:border-gray-200 p-2.5 rounded-[7px]' %>
                                      </div>
                                    </td>
                                    <td>
                                      <div class=""> <%= f.submit "log in", type: "image", src: asset_path("checkmark.png"), class: "mt-[5px] ml-[25px] submit_section" %> </div>
                                    </td>
                                  <% end %>
                                </tr>
                              </tbody>
                            </table>
                            <div class="flex">
                              <span class="text-[35px] cursor-pointer text-gray-500 text-center bg-[#F2F2F2] lg:w-[5%] md:w-[15%] sm:w-[25%] w-[35%] display-section-button">+</span>
                            </div>
                          </div>
                        </div>
                      </div>
                      <!-- End First Contnet -->
                      <!-- First Contnet -->
                      <div data-hs-stepper-content-item='{"index": 3, "isFinal": true}' style="display: none;">
                        <div class="p-4 h-[100%]">
                          <div class="relative overflow-x-auto mt-4 w-[90%] mx-auto h-[45vh]">
                            <p class="mb-4 text-[15px] text-[#000] font-medium">Course Name:</span>
                            <span class="text-[#818181] pl-3 font-medium text-[15px]"><%= Course.last.course_name %></span>
                            <span class="pl-3 text-[15px] text-[#000] font-medium">Section Name:</span>
                            <span class="text-[#818181] pl-3 font-medium text-[15px]"><%= Section.last.title %></span>
                          </p>
                          <table class="w-full text-sm text-left text-gray-500 dark:text-gray-400 overflow-auto table-auto">
                            <thead class="text-xs text-[#000] bg-[#EDEDED] dark:text-gray-400">
                              <tr>
                                <th scope="col" class="px-6 py-5 font-medium text-[18px]"> Title </th>
                                <th scope="col" class="px-6 py-5 font-medium text-[18px]"> Description </th>
                                <th scope="col" class="px-6 py-5 font-medium text-[18px]"> Clip           </th>
                                <th scope="col" class="px-6 py-5 font-medium text-[18px]"> Attachments </th>
                                <th scope="col" class="px-6 py-5 font-medium text-[18px]"> Action </th>
                              </tr>
                            </thead>
                            <%= render partial: 'admin/lessons/table', locals: { lessons: @lessons, section_id: @section_id } %>
                          </tbody>
                        </table>
                        <div class="flex display-lesson-button">
                          <span class="text-[35px] text-gray-500 text-center bg-[#F2F2F2] lg:w-[5%] md:w-[15%] sm:w-[25%] w-[35%] display-lesson-button">+</span>
                        </div>
                      </div>
                    </div>
                  </div>
                  <!-- End First Contnet -->
                  <!-- Final Contnet -->
                  <div data-hs-stepper-content-item='{"isFinal": true}' style="display: none;">
                    <div class="p-4 h-48 bg-gray-50 flex justify-center items-center border border-dashed border-gray-200 rounded-xl">
                      <h3 class="text-gray-500"> Final content </h3>
                    </div>
                  </div>
                  <!-- End Final Contnet -->
                  <!-- Button Group -->
                  <div class="mt-5 flex justify-end  items-center gap-x-2">
                    <button type="button" class="py-2 px-3 inline-flex items-center gap-x-1 text-sm font-medium rounded-lg border border-gray-200 bg-white text-gray-800 shadow-sm hover:bg-gray-50 disabled:opacity-50 disabled:pointer-events-none" data-hs-stepper-back-btn>
                      <svg class="flex-shrink-0 w-4 h-4" xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
                        <path d="m15 18-6-6 6-6" />
                      </svg>
                      Back
                    </button>
                    <button type="button" class="py-2 px-10 text-[#fff] inline-flex items-center gap-x-1 text-sm font-semibold rounded-lg border border-transparent bg-gradient-to-r from-[#1AB69D] to-[#31B978] disabled:opacity-50 disabled:pointer-events-none next-btn" data-hs-stepper-next-btn> Next </button>
                    <button type="button" class="py-2 px-3 inline-flex items-center gap-x-1 text-sm font-semibold rounded-lg border border-transparent bg-gradient-to-r from-[#1AB69D] to-[#31B978] text-white disabled:opacity-50 disabled:pointer-events-none" data-hs-stepper-finish-btn style="display: none;"> Finish </button>
                    <button type="reset" class="py-2 px-3 hidden items-center gap-x-1 text-sm font-semibold rounded-lg border border-transparent bg-gradient-to-r from-[#1AB69D] to-[#31B978] text-white disabled:opacity-50 disabled:pointer-events-none" data-hs-stepper-reset-btn style="display: none;"> Reset </button>
                  </div>
                  <!-- End Button Group -->
                </div>
                <!-- End Stepper Content -->
              </div>
              <!-- End Stepper -->
            </div>
          </div>
          <div class="w-[100%] md:w-[25%] h-[52.25rem] bg-[#DEDEDE] hidden vertical-stepper">
            <div class="w-[100%] h-[82vh] mt-4 mb-4 overflow-auto" id="sections-loop">
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
</div>
<div class="flex items-center justify-between pt-10 md:pt:20 pb-4 md:w-[88%] lg:w-[95%] w-[95%] mr-auto lg:ml-[30px] md:ml-[55px] ml-[30px] flex-col sm:flex-row md:flex-row">
  <div class="text-[#31B978] text-[18px] font-bold  w-[70%] md:w-[35%] pb-4 sm:pb-0">
    <%= collection_select(:course, :course_name, Course.all, :id, :course_name, { prompt: "Select Course", selected: '' }, { class: "bg-gray-50 border-0 foucus:ring-0 text-[#818181] text-[16px] font-medium rounded-lg outline-none block w-full p-2.5 rounded-[7px] bg-[#fff] text-[#818181] filter-handle custom-select sources" }) %>
  </div>
  <label for="table-search " class="sr-only">Search</label>
  <%= form_tag admin_courses_path, method: :get, id: "course_search_form" do %>
    <div class="relative ml-[45px] sm:ml-[50px] md:ml-[55px] lg:ml-[60px]">
      <div class="absolute inset-y-0 left-0 flex items-center pl-3 pointer-events-none">
        <img src="<%= asset_path("search.png") %>">
      </div>
      <%= text_field_tag :search, params[:search], id: "course_search", class: "block p-2 pl-10 text-sm text-gray-900 border-[0] focus:ring-0 rounded-lg md:w-72 h-11 bg-gray-50 dark:placeholder-gray-400 dark:text-white lg:w-full sm:w-[93%] w-[90%]", placeholder: "Search or type", autocomplete: "off" %>
    </div>
  <% end %>
</div>
<div class="relative overflow-x-auto lg:overflow-x-hidden mt-4 w-[87%] sm:w-[90%] md:w-[95%] lg:w-[95%] mr-auto ml-[30px] h-[50vh]">
  <table class="w-full text-sm text-left text-gray-500 ml-[47px] sm:ml-[63px] lg:ml-0 md:ml-[26px] dark:text-gray-400 overflow-auto table-auto">
    <thead class="text-[#000] bg-[#EDEDED] dark:text-gray-400">
      <tr>
        <th scope="col" class="px-6 py-5 font-medium text-[14px] sm:text-[14px] md:text-[16px] lg:text-[18px]">
          Course Name
        </th>
        <th scope="col" class="px-6 py-5 font-medium text-[14px] sm:text-[14px] md:text-[16px] lg:text-[18px]]">
          Description
        </th>
        <th scope="col" class="px-6 py-5 font-medium text-[14px] sm:text-[14px] md:text-[16px] lg:text-[18px]">
          Action 
        </th>
      </tr>
    </thead>
    <tbody class="overflow-auto filter-table-data" id="course-table">
      <%= render partial: "admin/courses/table", locals: { courses: @courses } %>
    </tbody>
  </table>
  <div id="large-modal" tabindex="-1" class="fixed top-0 left-0 right-0 z-[1000] hidden w-full p-4 overflow-x-hidden overflow-y-auto md:inset-0 h-[calc(100%-1rem)] max-h-full" style="backdrop-filter: contrast(0.5);">
    <div class="relative w-full max-w-4xl max-h-full">
      <!-- Modal content -->
      <div class="relative bg-white rounded-lg shadow dark:bg-gray-700">
        <!-- Modal header -->
        <div class="flex items-center justify-between p-4 md:p-5 rounded-t dark:border-gray-600">
          <h3 class="text-[26px] font-semibold text-[#31B978] text-center flex justify-center w-full pl-[50px]"> EDIT COURSE </h3>
          <button type="button" id="course-modal-close-btn" class="text-gray-400 bg-transparent hover:bg-gray-200 hover:text-gray-900 rounded-lg text-sm w-8 h-8 ms-auto inline-flex justify-center items-center dark:hover:bg-gray-600 dark:hover:text-white" data-modal-hide="large-modal">
            <svg class="w-3 h-3" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 14 14">
              <path stroke="#31B978" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="m1 1 6 6m0 0 6 6M7 7l6-6M7 7l-6 6" />
            </svg>
            <span class="sr-only">Close modal</span>
          </button>
        </div>
        <!-- Modal body -->
        <%= turbo_frame_tag "edit-course-popup" do %>
        <% end %>
      </div>
    </div>
  </div>
  <div id="popup-modal" tabindex="-1" class="fixed top-0 left-0 right-0 z-[1000] hidden p-4 overflow-x-hidden overflow-y-auto md:inset-0 h-[calc(100%-1rem)] max-h-full shadow-lg" style="backdrop-filter: contrast(0.5);">
    <div class="relative w-full max-w-[42rem] max-h-full shadow-md">
      <div class="relative bg-white rounded-lg shadow dark:bg-gray-700">
        <h4 class="text-[20px] font-medium pt-6 pl-6 text-[#31B978]">Confirm Deletion</h4>
        <button type="button" id="course-detele-modal-btn" class="absolute top-3 right-2.5 text-gray-400 bg-transparent hover:bg-gray-200 hover:text-gray-900 rounded-lg text-sm w-8 h-8 ml-auto inline-flex justify-center items-center dark:hover:bg-gray-600 dark:hover:text-white" data-modal-hide="popup-modal">
          <svg class="w-10 h-4" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 14 14">
            <path stroke="#0BC08B" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="m1 1 6 6m0 0 6 6M7 7l6-6M7 7l-6 6" />
          </svg>
          <span class="sr-only">Close modal</span>
        </button>
        <div class="px-6 pb-6 pt-2">
          <h3 class="mb-5 text-base text-[#818181] font-medium mt-4 ">Are you sure you want to delete this product?</h3>
          <div class="flex justify-start">
            <%= link_to 'DELETE', "", data: { turbo_method: :delete }, class: "w-[150px] h-[42px] mr-2 text-white bg-gradient-to-r from-[#1AB69D] to-[#31B978] font-medium rounded-lg text-[16px] block items-center px-9 py-2.5 text-center mr-2 delete-gradiant-hover", id:"delete-course-model" %>
            <button data-modal-hide="popup-modal" type="button" class="w-[150px] h-[42px] text-white rounded-lg border bg-gradient-to-r from-[#1AB69D] to-[#31B978] text-base font-medium px-9 py-2.5 focus:z-10 dark:bg-gray-700 dark:text-gray-300 dark:border-gray-500 dark:hover:text-white dark:hover:bg-gray-600 dark:focus:ring-gray-600 delete-gradiant-hover">CANCEL</button>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="mt-4">
    <%= will_paginate(@courses) %>
  </div>
</div>
<script>
  $(document).ready( function () {
      if (typeof HSStepper !== 'undefined') {
         setTimeout(function () {
        const stepperElement = HSStepper.getInstance('[data-hs-stepper]');
        console.log(stepperElement);

        if (stepperElement) {
          try {
            const stepperInstance = HSStepper.getInstance('[data-hs-stepper]');
            console.log(stepperInstance);

            $('[data-hs-stepper-next-btn]').on('click', function () {
              stepperInstance.on('next');

              const currentIndex = stepperInstance.currentIndex + 1;
              console.log(currentIndex);

              if (currentIndex === 3) {
                $.ajax({
                  url: "/admin/courses/sections/all",
                  type: "GET",

                  success: function (response) {},
                  error: function (xhr, status, error) {},
                });
                console.log("ajax called");
                $('[data-hs-stepper-next-btn]').css('display', 'none');
                $('[data-hs-stepper-finish-btn]').css('display', 'block');
                $(".reduce-course").addClass("reduce-course2");
                $(".vertical-stepper").css("display", "block");

              } else if (currentIndex === 4) {
                $('[data-hs-stepper-next-btn]').css('display', 'none');
                $('[data-hs-stepper-finish-btn]').css('display', 'block');
                $(".reduce-course").removeClass("reduce-course2");
                $(".vertical-stepper").css("display", "none");

              }
            });

          } catch (error) {
            console.error("Error initializing HSStepper:", error);
          }
        } else {
          console.error("Element with data-hs-stepper not found");
        }
      }, 1000);
      } else {
        console.error("HSStepper not defined. Make sure preline.js is loaded.");
      }
    });
</script>
