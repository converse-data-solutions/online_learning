<% @users.each do |user| %>
  <% if user.has_role?(:admin) %>
    <tr class="bg-white border-b dark:bg-gray-800 dark:border-gray-700 hover:bg-gray-50 dark:hover:bg-gray-600">
      <th scope="row" class="px-6 py-4 font-medium text-[12px] sm:text-[14px] md:text-[15px] text-[#818181] whitespace-nowrap dark:text-white">
        <%= user.id %>
      </th>
      <td class="px-6 py-4 font-medium text-[12px] sm:text-[14px] md:text-[15px] text-[#818181]">
        <%= user.name %>
      </td>
      <td class="px-6 py-4 font-medium text-[12px] sm:text-[14px] md:text-[15px] text-[#818181]">
        <%= user.email %>
      </td>
      <td class="px-6 py-4 font-medium text-[12px] sm:text-[14px] md:text-[15px] text-[#818181]">
        ***********
      </td>
      <td class="px-6 py-4 flex gap-4">
        <button data-modal-target="large-modal-<%= user.id %>" data-modal-toggle="large-modal-<%= user.id %>" class="block w-full md:w-auto   " type="button">
          <img src="<%= asset_path("pencil.png") %>" alt="...">
        </button>
        <div id="large-modal-<%= user.id %>" tabindex="-1" class="fixed top-0 left-0 right-0 z-[1000] hidden w-full p-4 overflow-x-hidden overflow-y-auto md:inset-0 h-[calc(100%-1rem)] max-h-full" style="backdrop-filter: contrast(0.5);">
          <div class="relative w-full lg:max-w-4xl sm:max-w-[32rem] max-h-full max-w-[20rem] ml-[42px] sm:ml-[25px] md:ml-[0px]">
            <!-- Modal content -->
            <div class="relative bg-white rounded-lg shadow dark:bg-gray-700">
              <!-- Modal header -->
              <div class="flex items-center justify-between p-4 md:p-5 rounded-t dark:border-gray-600">
                <h3 class="text-[26px] font-semibold text-[#31B978] text-center flex justify-center w-full">
                  Edit User Management
                </h3>
                <%= link_to admin_users_path do %>
                  <button type="button" class="text-gray-400 bg-transparent hover:bg-gray-200 hover:text-gray-900 rounded-lg text-sm w-8 h-8 ms-auto inline-flex justify-center items-center dark:hover:bg-gray-600 dark:hover:text-white" data-modal-hide="large-modal-<%= user.id %>">
                    <svg class="w-3 h-3" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 14 14">
                      <path stroke="#31B978" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="m1 1 6 6m0 0 6 6M7 7l6-6M7 7l-6 6"/>
                    </svg>
                    <span class="sr-only">Close modal</span>
                  </button>
                <% end %>
              </div>
              <!-- Modal body -->
              <div class="p-7 space-y-6 lg:w-[86%] sm:w-[95%] w-[90%] mr-auto lg:ml-10 ml-5">
                <%= render partial: 'admin/users/edit', locals: { user: user } %>
              </div>
            </div>
          </div>
        </div>
        <button data-modal-target="popup-modal-<%= user.id %>" data-modal-toggle="popup-modal-<%= user.id %>" class="block" type="button">
          <img src="<%= asset_path("delete.png") %>" alt="...", class="del-img">
        </button>
        <div id="popup-modal-<%= user.id %>" tabindex="-1" class="fixed top-0 left-0 right-0 z-[1000] hidden p-4 overflow-x-hidden overflow-y-auto md:inset-0 h-[calc(100%-1rem)] max-h-full shadow-lg" style="backdrop-filter: contrast(0.5);">
          <div class="relative w-full max-w-[42rem] max-h-full shadow-md">
            <div class="relative bg-white rounded-lg shadow dark:bg-gray-700">
              <h4 class="text-[20px] font-medium pt-6 pl-6 text-[#31B978]">Confirm Deletion</h4>
              <button type="button" class="absolute top-3 right-2.5 text-gray-400 bg-transparent hover:bg-gray-200 hover:text-gray-900 rounded-lg text-sm w-8 h-8 ml-auto inline-flex justify-center items-center dark:hover:bg-gray-600 dark:hover:text-white" data-modal-hide="popup-modal-<%= user.id %>">
                <svg class="w-3 h-3" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 14 14">
                  <path stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="m1 1 6 6m0 0 6 6M7 7l6-6M7 7l-6 6"/>
                </svg>
                <span class="sr-only">Close modal</span>
              </button>
              <div class="px-6 pb-6 pt-2">
                <h3 class="mb-5 text-base font-medium text-[#818181] dark:text-gray-400 mt-4 ">Are you sure you want to delete this product?</h3>
                <%= link_to 'DELETE', admin_user_path(user), data: {turbo_method: :delete}, class: "mr-2 text-white bg-gradient-to-r from-[#1AB69D] to-[#31B978] font-medium rounded-lg text-[16px] inline-flex items-center px-9 py-2.5 text-center mr-2" %>
                <button data-modal-hide="popup-modal" type="button" class="text-white rounded-lg border bg-gradient-to-r from-[#1AB69D] to-[#31B978] text-sm font-medium px-9 py-2.5 focus:z-10 dark:bg-gray-700 dark:text-gray-300 dark:border-gray-500 dark:hover:text-white dark:hover:bg-gray-600 dark:focus:ring-gray-600">CANCEL</button>
              </div>
            </div>
          </div>
        </div>
      </td>
    </tr>
  <% end%>
<% end %>
